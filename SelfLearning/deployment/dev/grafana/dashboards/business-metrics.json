{
  "id": null,
  "title": "Business Metrics",
  "tags": [
    "business",
    "api"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Q&A Requests Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(qa_requests_total[5m])) by (status)",
          "legendFormat": "{{status}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests/sec"
        },
        {
          "format": "short"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 6,
      "title": "Q&A Tokens Used",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(rate(qa_tokens_used_total[5m])) by (publisher, model)",
          "legendFormat": "{{publisher}} - {{model}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Tokens/sec"
        },
        {
          "format": "short"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 2,
      "title": "Q&A Success Rate",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 8,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(qa_requests_total{status=\"success\"}[5m])) / sum(rate(qa_requests_total[5m])) * 100",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 90,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "green"
              }
            ]
          }
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 3,
      "title": "Total Q&A Requests",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(qa_requests_total[1h]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 4,
      "title": "Q&A Processing Duration",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(qa_processing_duration_seconds_bucket[5m])) by (le, publisher))",
          "legendFormat": "P50 {{publisher}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(qa_processing_duration_seconds_bucket[5m])) by (le, publisher))",
          "legendFormat": "P95 {{publisher}}",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(qa_processing_duration_seconds_bucket[5m])) by (le, publisher))",
          "legendFormat": "P99 {{publisher}}",
          "refId": "C",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Duration"
        },
        {
          "format": "short"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 5,
      "title": "Q&A Answer Word Count",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(qa_answer_word_count_bucket[5m])) by (le, publisher))",
          "legendFormat": "P50 {{publisher}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(qa_answer_word_count_bucket[5m])) by (le, publisher))",
          "legendFormat": "P95 {{publisher}}",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "avg(qa_answer_word_count_sum / qa_answer_word_count_count) by (publisher)",
          "legendFormat": "Avg {{publisher}}",
          "refId": "C",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "none",
          "label": "Words"
        },
        {
          "format": "short"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 8,
      "title": "Similarity Searches",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(rate(similarity_searches_total[5m])) by (status)",
