      "id": 1,
      "title": "Questions Generated Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(worker_questions_generated_total[5m])) by (publisher_domain)",
          "legendFormat": "{{publisher_domain}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "ops",
          "label": "Questions/sec"
        },
        {
          "format": "short"
        }
